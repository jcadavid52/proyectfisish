
@{
    ViewBag.Title = "IniciarSesion";
}

<h2>IniciarSesion</h2>

<form action="@Url.Action("IniciarSesionAccion","Principal")" method="post">
    <label>Ingrese usuario</label><br /><br />
    <input type="text" name="CORREO" value="" /><br /><br />
