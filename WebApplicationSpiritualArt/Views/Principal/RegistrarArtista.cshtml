@model List<WebApplicationSpiritualArt.Models.TIPO_PLAN>
   
@{
    ViewBag.Title = "RegistrarArtista";
    
}

<h2>RegistrarArtista</h2>

<form action="@Url.Action("RegistrarArtistaAccion","Principal")" method="post">
    
    <input type="hidden" name="IMAGEN" value="~/ArchivosLectura/images.jpg" />
    <label>NOMBRE</label><br />
    <input type="text" name="NOMBRE_ARTISTA" required /><br /><br />
    <label>PRIMER APELLIDO</label><br />
    <input type="text" name="PRIMER_APELLIDO_ARTISTA" required /><br /><br />
    <label>SEGUNDO APELLIDO</label><br />
    <input type="text" name="SEGUNDO_APELLIDO_ARTISTA" required /><br /><br />
    <label>TELÉFONO</label><br />
    <input type="text" name="TELEFONO" required /><br /><br />
    <label>CORREO</label><br />
    <input type="email" name="CORREO" required /><br /><br />
    <label>PAÍS</label><br />
    <input type="text" name="PAIS" required /><br /><br />
    <label>CIUDAD</label><br />
    <input type="text" name="CIUDAD" required /><br /><br />
    <label>LOCALIDAD</label><br />
    <input type="text" name="LOCALIDAD" required /><br /><br />
    <label>DIRECCIÓN</label><br />
    <input type="text" name="DIRECCION" required /><br /><br />
    <label>Tipo de plan</label><br />
    <select name="FK_TIPO_PLAN">
        @foreach (var planes in Model)
        {
            <option value="@planes.PK_ID_TIPO_PLAN">@planes.NOMBRE_PLAN</option>
        }

    </select><br /><br />

    <input type="submit" value="Registrarse" />
</form>


